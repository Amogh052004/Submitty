{
    "required_capabilities": "default",
    "grading_parameters": {
        "TOTAL_POINTS": 10,
        "AUTO_POINTS": 10,
        "TA_POINTS": 0,
        "EXTRA_CREDIT_POINTS": 0
    },
    "autograding_method": "docker",
    "autograding": {
        "submission_to_compilation": ["**/*.v"],
        "compilation_to_runner": ["**/a.out"]
        //"compilation_to_validation": ["**/STDOUT*.txt","**/STDERR*.txt","**/a.out"]
    },
    "container_options": {
        "container_image": "tkoz0/submitty-verilog"
    },
    "testcases": [
        {
            "type": "Compilation",
            "title": "Compilation",
            "command": "iverilog *.v",
            "executable_name": "a.out",
            "points": 5
        },
        {
            "title": "Check output",
            "details": "details",
            "command": "./a.out",
            "points": 5,
            "validation": [
                {
                    "actual_file": "STDOUT.txt",
                    "method": "diff",
                    "comparison": "byLinebyChar",
                    "expected_string": "hello world\n"
                }
            ]
        }
    ]
}
